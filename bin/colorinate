#!/usr/bin/env python3
import sys
from itertools import cycle, groupby

COLORS = [
    "#4e9a06",
    "#386cb0",
    "#c4a800",
    "#c81f1f",
    "#6050b0",
    "#06989a",
    "#c8742a",
    "#80487f",
    "#6b6d68",
]


def iter_colors():
    for color in cycle(COLORS):
        r = int(color[1:3], 16)
        g = int(color[3:5], 16)
        b = int(color[5:7], 16)
        yield f"\x1b[38;2;{r};{g};{b}m"


def key(line):
    return line.split(":", maxsplit=1)[0]


for color, (key, lines) in zip(iter_colors(), groupby(sys.stdin, key=key)):
    for line in lines:
        line = line.rstrip("\n")
        print(f"{color}{line}\x1b[0m")
